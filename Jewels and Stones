class Solution:
    def numJewelsInStones(self, J, S):
        """
        :type J: str
        :type S: str
        :rtype: int
        """
        jmap = set()
        for c in J:
            jmap.add(c)
        count = 0
        for c in S:
            if c in jmap:
                count+=1
        return count
